source":"toggle-switchy.css","name":null,"original":{"line":322,"column":2},"generated":{"line":1,"column":5659}},{"source":"toggle-switchy.css","name":null,"original":{"line":323,"column":0},"generated":{"line":1,"column":5674}},{"source":"toggle-switchy.css","name":null,"original":{"line":324,"column":0},"generated":{"line":1,"column":5675}},{"source":"toggle-switchy.css","name":null,"original":{"line":325,"column":2},"generated":{"line":1,"column":5738}},{"source":"toggle-switchy.css","name":null,"original":{"line":326,"column":0},"generated":{"line":1,"column":5755}}],"sources":{"toggle-switchy.css":"/*\nToggle Switchy v1.13\nby Adam Culpepper | @adamculpepper\nhttps://toggleswitchy.com\n*/\n\n/* Customizable styles */\n\n/* Colors: Default (blue) */\n.toggle-switchy {\n  color: #fff;\n}\n.toggle-switchy > input + .toggle:before {\n  content: \"째C\";\n}\n.toggle-switchy > input + .toggle:after {\n  content: \"째F\";\n}\n.toggle-switchy > input + .toggle > .switch {\n  background: #fff;\n}\n.toggle-switchy > input + .toggle + .label {\n  color: #000;\n}\n.toggle-switchy > input:checked + .toggle {\n  background: #3498db;\n}\n.toggle-switchy > input:not(:checked) + .toggle {\n  background: #ccc;\n}\n.toggle-switchy > input:checked + .toggle > .switch {\n  border: 3px solid #3498db;\n}\n.toggle-switchy > input:not(:checked) + .toggle > .switch {\n  border: 3px solid #ccc;\n}\n.toggle-switchy > input:focus + .toggle,\n.toggle-switchy > input:active + .toggle {\n  box-shadow: 0 0 5px 3px rgba(0, 119, 200, 0.5);\n}\n\n/* Rounded switch corners */\n.toggle-switchy > input + .toggle {\n  border-radius: 4px;\n}\n.toggle-switchy > input + .toggle .switch {\n  border-radius: 6px;\n}\n\n/* //////////////////////////\nCORE STYLES BELOW - NO TOUCHY\n////////////////////////// */\n.toggle-switchy {\n  display: inline-flex;\n  align-items: center;\n  user-select: none;\n  position: relative;\n  vertical-align: middle;\n  margin-bottom: 0;\n}\n.toggle-switchy:hover {\n  cursor: p